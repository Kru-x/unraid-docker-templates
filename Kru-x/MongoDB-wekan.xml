<?xml version="1.0"?>
<Container version="2">
  <Name>MongoDB-wekan</Name>
  <Project>https://www.mongodb.com/</Project>
  <Repository>mongo</Repository>
  <Registry>https://hub.docker.com/_/mongo/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support/>
  <Category/>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://rawgit.com/Kru-x/unraid-docker-templates/master/Kru-x/img/mongo.sh-600x600.png</Icon>
  <ExtraParams/>
  <DateInstalled>1475966264</DateInstalled>
  <Description> MongoDB document databases provide high availability and easy scalability.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/MongoDB/</HostDir>
      <ContainerDir>/data/db</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Config Name="AppData Config Path" Target="/data/db" Default="" Mode="rw" Description="Where to store the database/wekan's data (backup this folder if it is stored on the cache drive only)" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/MongoDB/</Config>
  <Config Name="Variable 1" Target="PUID" Default="" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="Variable 2" Target="PGID" Default="" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
</Container>
