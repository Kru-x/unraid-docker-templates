<?xml version="1.0"?>
<Container version="2">
  <Name>MongoDB-wekan</Name>
  <Repository>mongo</Repository>
  <Registry>https://hub.docker.com/_/mongo/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support/>
  <Overview/>
  <Category/>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://rawgit.com/Kru-x/unraid-docker-templates/master/Kru-x/img/mongo.sh-600x600.png</Icon>
  <ExtraParams/>
  <DateInstalled>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/MongoDB/</HostDir>
      <ContainerDir>/data/db</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Config Name="AppData Config Path" Target="/data/db" Default="" Mode="rw" Description="Where to store the database/wekan's data (backup this folder if it is stored on the cache drive only)" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/MongoDB/</Config>
</Container>
